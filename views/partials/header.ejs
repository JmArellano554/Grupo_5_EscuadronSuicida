<header class="contenedorSuperior">

  <div id="mySidenav" class="sidenav">

        <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
        <a href="/">Inicio</a>
        <a href="/products/productcart">Tienda</a>
        <a href="/products/">Productos</a>
        <a href="/users/register">Registrarse</a>
        <a href="/users/login">Iniciar Sesión</a>
        
        <a href="#">Contacto</a>
      </div>

  <span class="btnMenu" onclick="openNav()">&#9776;</span>

    <script>
    function openNav() {
      document.getElementById("mySidenav").style.width = "35vw";
    }
    
    function closeNav() { 
      document.getElementById("mySidenav").style.width = "0vw";
    }
    </script>
  <div class="contenedor-buscador">
          <input type="search" placeholder="Buscar un producto">
         </div>
    <div class="imagenLogo"> 
      <a href="/"><img class="logoHeader" src="\imagenes\logo.png" alt="logo"></a>
    </div>
    <div class="registro-formulario">
      <ul class="navbar-nav align-items-center">
				<% if (locals.isLogged) { %>
				  	hola <%=locals.userLogged.first_name%>
            
            <a href="/users/profile">Mi Perfil</a>
					
            <a href="/users/logout">Cerrar Sesión</a>
						</ul>
					</li>
				<% } else { %>
          <span><a href="/users/register">Registrarse</a></span>
          <span> <a href="/users/login">Iniciar Sesión</a></span>
        <% } %>
      </ul>
    </div>  
    <div class="imagenCarrito">
       <a href="/views/products/productCart.ejs"><i class="fab fa-opencart"></i></a>
    </div>
        
</header>